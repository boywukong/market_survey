<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ContentActivity">

    <ImageView
        android:id="@+id/imageView_luyin"
        android:layout_width="151dp"
        android:layout_height="206dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_paizhao"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/btn_luyin_770x180" />

    <ImageView
        android:id="@+id/imageView_paizhao"
        android:layout_width="150dp"
        android:layout_height="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_luyin"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/btn_paizhao_770x180" />

    <ImageView
        android:id="@+id/imageView_baocun"
        android:layout_width="151dp"
        android:layout_height="214dp"
        android:layout_marginTop="500dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView_paizhao"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/btn_baocun_770x180" />

    <ImageView
        android:id="@+id/imageView_dayin"
        android:layout_width="144dp"
        android:layout_height="216dp"
        android:layout_marginTop="500dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toEndOf="@+id/imageView_baocun"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/btn_dayin_770x180" />
</androidx.constraintlayout.widget.ConstraintLayout>